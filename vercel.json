{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/dashboard", "dest": "/dashboard.html" },
    { "src": "/support", "dest": "/support.html" },
    { "src": "/settings", "dest": "/settings.html" },
    { "src": "/profile", "dest": "/profile.html" },
    { "src": "/withdrawal", "dest": "/withdrawal.html" },
    { "src": "/links", "dest": "/links.html" },
    { "src": "/notifications", "dest": "/notifications.html" },
    { "src": "/announcements", "dest": "/announcements.html" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
